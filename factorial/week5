#include<stdio.h>
int factorial(int n);
int main() {
    int n,c;
    printf("Enter a positive integer: ");
    scanf("%d",&n);
    c=factorial(n);
    printf("Factorial of %d = %d", n, c);
}

 int factorial(int n)
 {
    int f=1;
    if (n>0)
    {
        f= n*factorial(n-1);
 }
        return f;
}

